{
  "name": "keycode-info",
  "homepage": "https://uiwjs.github.io/keycode-info",
  "version": "1.1.0",
  "description": "A simple web page that responds to the pressed key and returns information about the JavaScript'on-key press' key.",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "start": "kkt start",
    "build": "kkt build",
    "test": "kkt test --env=jsdom",
    "test:coverage": "kkt test --env=jsdom --coverage --bail",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/keycode-info.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@uiw/react-copy-to-clipboard": "~4.12.2",
    "@uiw/react-github-corners": "~1.5.3",
    "@wcj/dark-mode": "~1.0.13",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-json-view": "~1.21.3"
  },
  "devDependencies": {
    "@kkt/less-modules": "~7.1.1",
    "@types/react": "~17.0.39",
    "@types/react-dom": "~17.0.11",
    "husky": "~7.0.4",
    "kkt": "~7.1.5",
    "lint-staged": "~12.3.4",
    "prettier": "~2.8.0"
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "coverageReporters": [
      "lcov",
      "json-summary"
    ],
    "testMatch": [
      "<rootDir>/src/**/*.test.{ts,tsx}"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/app/*.{tsx,ts}"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
